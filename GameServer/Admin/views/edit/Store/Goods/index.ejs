<div class="pagetitle">
    <a href="/Goods/add">添加物品</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">商品编号</td>
        <td class="title">商品名称</td>
        <td class="title">商品排序</td>
        <td class="title">单价</td>
        <td class="title">商店</td>
        <td class="title">创建日期</td>
        <td class="title">有效</td>

    </tr>
    <%
    //if(results){
    results.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/Goods/edit?id=<%-item.Goods_Id%>"><input type="submit" name="submit" id="submit" value="编辑" /></a></td>
        <td class="data"><%-item.Goods_Id%></td>
        <td class="data"><%-item.Item_Name%>*<%-item.Item_Amount%></td>
        <td class="data"><%-item.Goods_Order%></td>
        <td class="data"><%-item.Price%>(<%-item.Currency_Name%>)</td>
        <td class="data"><%-item.Store_Name%></td>
        <td class="data"><%-item.Create_Date.format('yyyy-MM-dd hh:mm:ss')%></td>
        <td class="data"><%-item.Enable%></td>
    </tr>
    <%
    });
    %>
</table>