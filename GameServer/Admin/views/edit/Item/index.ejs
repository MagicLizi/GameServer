<div class="pagetitle">
    <a href="/Item/add">添加物品</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">编号</td>
        <td class="title">排序</td>
        <td class="title">名称</td>
        <td class="title">类型</td>
        <td class="title">创建日期</td>
    </tr>
    <%
    //if(results){
    results.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/Item/edit?id=<%-item.Item_Id%>"><input type="submit" name="submit" id="submit" value="编辑" /></a></td>
        <td class="data"><%-item.Item_Id%></td>
        <td class="data"><%-item.Item_Order%></td>
        <td class="data"><%-item.Item_Name%></td>
        <td class="data"><%
            switch (item.Item_Type){
            case enums.Item_Type.Currency:
            %>货币<%
                break;
                case enums.Item_Type.Fragment:
            %>碎片<%
                break;
                case enums.Item_Type.Scroll:
            %>卷轴<%
                break;
                case enums.Item_Type.SoulStone:
            %>灵魂石<%
                break;
                case enums.Item_Type.Consumables:
            %>消耗品<%
                break;
                case enums.Item_Type.Equipment:
            %>装备<%
            }%></td>
        <td class="data"><%-item.Create_Date.format('yyyy-MM-dd hh:mm:ss')%></td>
    </tr>
    <%
    });
    //}
    %>
</table>