<div class="pagetitle">
    <a href="/Item/Effect/add?id=<%-results.Item_Id%>">添加效果</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">编号</td>
        <td class="title">效果类型</td>
        <td class="title">效果数值</td>
        <td class="title">创建日期</td>
    </tr>
    <%
    //if(results){
    results.result.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/Item/Effect/edit?id=<%-item.Relation_Id%>"><input type="submit" name="submit" id="submit" value="编辑" /></a>
            <a href="/Item/Effect/del?id=<%-item.Relation_Id%>&itemid=<%-item.Item_Id%>"><input type="submit" name="submit" id="submit" value="删除" /></a></td>
        <td class="data"><%-item.Relation_Id%></td>
        <td class="data"><%-item.Effect_Name%></td>
        <td class="data"><%-item.Effect_Value%></td>
        <td class="data"><%-item.Create_Date.format('yyyy-MM-dd hh:mm:ss')%></td>
    </tr>
    <%
    });
    //}
    %>
</table>