<script type="text/javascript">
    $(function () {
        $('#sd').datetimepicker();
        $('#ed').datetimepicker();
    });
</script>
<div class="pagetitle">
<form id="form1" name="form1" method="post" action="/redeemCodeSearch/search">
     兑换码:<input type="text" name="code" id="code" value = <%-code%>>
     <input type="submit" name="search" id="search" value="提交" />
</form>
</div>
<div class="pagetitle">
<form id="form2" name="form2" method="post" action="/redeemCodeSearch/searchPackage">
     礼包:<input type="text" name="package_id" id="package_id" value = <%-package_id%>>
     <input type="submit" name="searchPackage" id="searchPackage" value="提交" />
</form>
</div>
<table width="100%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">兑换码</td>
        <td class="title">用户id</td>
        <td class="title">昵称</td>
        <td class="title">对应礼包</td>
        <td class="title">兑换时间</td>
        <td class="title">是否成功</td>
    </tr>
<%
    results.forEach(function(item){
%>
<tr>
    <td class="data"><%-item.redeem_code%></td>
    <td class="data"><%-item.user_id%></td>
    <td class="data"><%-item.nickname%></td>
    <td class="data"><%-item.package_name%></td>
    <td class="data"><%-item.create_date.format('yyyy-MM-dd hh:mm:ss')%></td>
    <td class="data"><%-item.success%></td>
</tr>
<%
});
%>

    </td>
</table>

<table width="100%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">兑换码</td>
        <td class="title">使用次数</td>
        <td class="title">当前可用次数</td>
    </tr>
<%
    results2.forEach(function(item){
%>
<tr>
    <td class="data"><%-item.redeem_code%></td>
    <td class="data"><%-item.used_count%></td>
    <td class="data"><%-item.count%></td>

</tr>
<%
});
%>

    </td>
</table>