<div class="pagetitle">
    <a href="/monster/add">添加怪物</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">编号</td>
        <td class="title">名称</td>
        <td class="title">品质</td>
        <td class="title">创建日期</td>
    </tr>
    <%
    results.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/monster/edit?id=<%-item.recharge_id%>"><input type="submit" name="submit" id="submit" value="编辑" /></a></td>
        <td class="data"><%-item.recharge_id%></td>
        <td class="data"><%-item.name%></td>
        <td class="data"><%-item.quality%></td>
        <td class="data"><%-item.create_date.format('yyyy-MM-dd hh:mm:ss')%></td>
    </tr>
    <%
    });
    %>
</table>