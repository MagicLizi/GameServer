<div class="pagetitle">
    <a href="/lottery/pool_Item/add?id=<%-results.Pool_Id%>&Pool_Type=<%-results.Pool_Type%>">添加奖池物品</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">奖池编号</td>
        <td class="title">道具名称</td>
        <td class="title">道具数量</td>
        <td class="title">奖池品质</td>
        <td class="title">创建日期</td>
    </tr>
    <%
    results.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/lottery/pool_Item/edit?id=<%-item.Pool_Item_Id%>&Pool_Type=<%-results.Pool_Type%>">
                <input type="submit" name="submit" id="submit" value="编辑" /></a>
            <a href="/lottery/pool_Item/del?id=<%-item.Pool_Item_Id%>&Pool_Type=<%-results.Pool_Type%>&Pool_Id=<%-results.Pool_Id%>">
                <input type="submit" name="submit" id="submit" value="删除" /></a></td>
        <td class="data"><%-item.Pool_Id%></td>
        <td class="data"><%-item.Pool_Item_Name%></td>
        <td class="data"><%-item.Item_Amount%></td>
        <td class="data"><%-item.Pool_Quality%></td>
        <td class="data"><%-item.Create_Date.format('yyyy-MM-dd hh:mm:ss')%></td>
    </tr>
    <%
    });
    %>
</table>