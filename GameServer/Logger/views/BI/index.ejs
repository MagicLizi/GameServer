<div class="pagetitle">
    <a href="/LoggerBI/add">添加BI</a>
</div>
<table width="99%" border="0" align="center" cellpadding="0" cellspacing="1" bgcolor="#c0de98">
    <tr>
        <td class="title">编辑</td>
        <td class="title">编号</td>
        <td class="title">名称</td>
        <td class="title">创建日期</td>
        <td class="title">有效</td>
        <td class="title">执行</td>
    </tr>
    <%
    results.forEach(function(item){
    %>
    <tr>
        <td class="data"><a href="/LoggerBI/edit?id=<%-item.BI_Id%>"><input type="submit" name="submit" id="submit" value="编辑" /></a></td>
        <td class="data"><%-item.BI_Id%></td>
        <td class="data"><%-item.BI_Name%></td>
        <td class="data"><%-item.Create_Date.format('yyyy-MM-dd hh:mm:ss')%></td>
        <td class="data"><%if(item.Enable == 1){%>是<%}else{%>否<%}%></td>
        <td class="data"><%if(item.Enable == 1){%><a href="/LoggerBI/run?id=<%-item.BI_Id%>"><input type="submit" name="submit" id="submit" value="运行" /></a><%}%></td>
    </tr>
    <%
    });
    %>
</table>